{
  "name": "ChatterinoWatch",
  "version": "1.0",
  "description": "Reports the currently watched Twitch channel to Chatterino.",
  "permissions": ["nativeMessaging", "tabs", "storage"],
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "key": ""
}
